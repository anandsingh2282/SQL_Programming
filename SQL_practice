# create table for problem 1 , problem 1
CREATE TABLE `countries` (
  `COUNTRY_ID` int NOT NULL,
  `COUNTRY_NAME` varchar(10) DEFAULT NULL,
  `REGION_ID` varchar(10) DEFAULT NULL
);


